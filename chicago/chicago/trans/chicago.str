module chicago

imports
  
  completion/completion
  pp
  outline
  analysis
  renaming

rules // Debugging
  
  debug-show-aterm:
    (node, _, _, path, project-path) -> (filename, result)
    with
      filename := <guarantee-extension(|"aterm")> path
    ; result   := node
    
  rename-menu-action : menu-terms -> <rename-action(construct-textual-change, editor-analyze)> menu-terms    
